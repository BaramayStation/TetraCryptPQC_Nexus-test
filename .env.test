# TetraCryptPQC Test Environment Configuration
# TOP SECRET//COSMIC//THAUMIEL

# StarkNet Test Configuration
STARKNET_NETWORK=testnet
STARKNET_ADMIN_ADDRESS=0x123...
STARKNET_PRIVATE_KEY=0x456...

# Security Clearance
SECURITY_CLEARANCE_LEVEL=5

# Hardware Security Module
HSM_PROVIDER=YubiKey
HSM_PIN=123456
HSM_SLOT=0
HSM_PROTOCOL=FIDO2

# Quantum Key Distribution
QKD_SIMULATION_MODE=true
QKD_KEY_SIZE=1024
QKD_PROTOCOL=BB84
QKD_ERROR_RATE=0.01
QKD_SECURITY_PARAMETER=128

# OpenFHE Configuration
OPENFHE_SECURITY_LEVEL=256
OPENFHE_SCHEME=CKKS
OPENFHE_MULTIPLICATIVE_DEPTH=3
OPENFHE_SCALE_FACTOR_BITS=40

# IPFS/Web3.Storage
IPFS_GATEWAY=http://localhost:8080
WEB3_STORAGE_TOKEN=test_token
WEB3_STORAGE_ENDPOINT=https://api.web3.storage

# Post-Quantum Cryptography
PQC_ALGORITHM_KEM=ML-KEM-1024
PQC_ALGORITHM_SIG=SLH-DSA
PQC_HASH_ALGORITHM=SHAKE-256
PQC_SYM_ALGORITHM=AES-256-GCM

# Compliance Settings
FIPS_MODE=true
CMMC_LEVEL=2
FEDRAMP_IMPACT_LEVEL=high
DISA_STIG_COMPLIANCE=true

# Test Contract Addresses
CONTRACT_ADDRESS_TESTNET=0x789...
CONTRACT_ADDRESS_MAINNET=0xabc...

# Test User Accounts
TEST_USER_ADDRESS=0xdef...
TEST_USER_PRIVATE_KEY=0xghi...

# Security Monitoring
AUDIT_LOG_LEVEL=debug
THREAT_DETECTION_ENABLED=true
AI_SECURITY_ENABLED=true
QUANTUM_TAMPER_DETECTION=true

# Test Timeouts (ms)
TEST_TIMEOUT=30000
QUANTUM_OPERATION_TIMEOUT=5000
HSM_OPERATION_TIMEOUT=3000
BLOCKCHAIN_OPERATION_TIMEOUT=10000

# Test Coverage Thresholds
COVERAGE_BRANCHES=90
COVERAGE_FUNCTIONS=90
COVERAGE_LINES=90
COVERAGE_STATEMENTS=90
