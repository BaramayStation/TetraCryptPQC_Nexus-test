# TetraCryptPQC StarkNet Configuration
# TOP SECRET//COSMIC//THAUMIEL

# StarkNet Network (testnet or mainnet)
STARKNET_NETWORK=testnet

# StarkNet Admin Address (hex format)
STARKNET_ADMIN_ADDRESS=0x123...

# StarkNet Private Key (hex format)
STARKNET_PRIVATE_KEY=0x456...

# Security Clearance Level (1-5)
SECURITY_CLEARANCE_LEVEL=5

# Hardware Security Module Configuration
HSM_PROVIDER=YubiKey
HSM_PIN=123456

# Quantum Key Distribution Settings
QKD_SIMULATION_MODE=true
QKD_KEY_SIZE=1024
QKD_PROTOCOL=BB84

# OpenFHE Configuration
OPENFHE_SECURITY_LEVEL=256
OPENFHE_SCHEME=CKKS

# Contract Addresses
CONTRACT_ADDRESS_TESTNET=0x789...
CONTRACT_ADDRESS_MAINNET=0xabc...

# Environment Variables for Secure Deployment

# Database Configuration
DB_HOST=your_database_host
DB_PORT=your_database_port
DB_NAME=your_database_name
DB_USER=your_database_user
DB_PASSWORD=your_database_password

# Cryptographic Keys
ENCRYPTION_KEY=your_encryption_key
SIGNATURE_KEY=your_signature_key

# API Keys
API_KEY=your_api_key

# Logging Configuration
LOG_LEVEL=info
LOG_FILE=logs/app.log

# Security Settings
ENABLE_THREAT_SCAN=true
MAX_LOGIN_ATTEMPTS=5
LOCKOUT_DURATION=300
