# AI Security Tools Configuration

# MITRE Caldera
CALDERA_API_KEY=your_caldera_api_key
CALDERA_ENDPOINT=http://caldera-service:8888
CALDERA_ADMIN=admin
CALDERA_PASSWORD=strongpassword
CALDERA_PORT=8888

# OpenAI CLIP
CLIP_MODEL=ViT-B/32
CLIP_THRESHOLD=0.85
CLIP_LOG_LEVEL=INFO

# DeepExploit
DEEPEXPLOIT_SCAN_INTERVAL=3600
DEEPEXPLOIT_TARGET_NETWORK=10.0.0.0/24
DEEPEXPLOIT_TARGET=localhost
DEEPEXPLOIT_PORT=8080
DEEPEXPLOIT_MODE=test

# AI-HIDS
AIHIDS_LOG_PATH=/var/log/aihids
AIHIDS_ALERT_THRESHOLD=5
AIHIDS_THRESHOLD=0.9
AIHIDS_LOG_FILE=/var/log/aihids.log

# TensorFlow Privacy
TF_PRIVACY_EPSILON=1.0
TF_PRIVACY_DELTA=1e-5
TF_PRIVACY_MAX_GRAD_NORM=1.0
